<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/Usuario_modificar_text"
        android:layout_width="116dp"
        android:layout_height="33dp"
        android:layout_above="@+id/Usuario_modificar"
        android:layout_alignStart="@+id/Usuario_modificar"
        android:layout_marginBottom="-134dp"
        android:textColor="@android:color/background_dark"
        android:textSize="16sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/Usuario_modificar"
        android:layout_width="228dp"
        android:layout_height="55dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="134dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        android:textColor="@android:color/background_dark"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/Constrase単a_modificar_text"
        android:layout_width="116dp"
        android:layout_height="29dp"
        android:layout_above="@+id/Constrase単a_modificar"
        android:layout_alignStart="@+id/Constrase単a_modificar"
        android:textColor="@android:color/background_dark"
        android:textSize="16sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/Constrase単a_modificar"
        android:layout_width="233dp"
        android:layout_height="59dp"
        android:layout_centerInParent="true"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="" />

    <Button
        android:id="@+id/Boton_modificar"
        android:layout_width="237dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="154dp"
        android:text="MODIFICAR URUSARIO" />

</RelativeLayout>
